import java.util.*;
class StringAlgo
{
	public static void main(String args[])
	{
		Scanner s = new Scanner(System.in);
		System.out.println("Enter your String: ");
		String st = s.next();
		
		String s1 = " ";
		String s2 = " ";
		
		for(int i=0; i<st.length(); i++)
		{
			if(st.charAt(i) == '-')
			{
				s1+=st.charAt(i);
			}
			else
			{
				s2+=st.charAt(i);
			}
		}
		System.out.println(s1+s2);

		System.out.println(s2+s1);

	}
}
