import java.util.*;
class LinearSearch
{
	public static void main(String args[])
	{
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int a[] = new int[n];
		for(int i=0; i<n; i++)
		{
			a[i]=s.nextInt();
		}
		int key = s.nextInt();
		for(int i=0; i<n; i++)
		{
			if(a[i]==key)	
			{
				System.out.println(i);
				break;
			}
		}
	}
}
